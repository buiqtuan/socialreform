# Test Environment Configuration
# Copy this file to .env.test and update with your test database credentials

# Database (should be a separate test database)
DATABASE_URL="postgresql://username:password@localhost:5432/social_reform_test"

# JWT Configuration
JWT_SECRET="test-jwt-secret-key-for-integration-tests"
JWT_REFRESH_SECRET="test-jwt-refresh-secret-key-for-integration-tests"

# Firebase Configuration (can use test project or mock)
FIREBASE_PROJECT_ID="test-project-id"
FIREBASE_PRIVATE_KEY="test-private-key"
FIREBASE_CLIENT_EMAIL="test@test-project.iam.gserviceaccount.com"

# Social Auth Test Configuration
TWITTER_CLIENT_ID="test-twitter-client-id"
TWITTER_CLIENT_SECRET="test-twitter-client-secret"

FACEBOOK_APP_ID="test-facebook-app-id"
FACEBOOK_APP_SECRET="test-facebook-app-secret"

GOOGLE_CLIENT_ID="test-google-client-id"
GOOGLE_CLIENT_SECRET="test-google-client-secret"

INSTAGRAM_CLIENT_ID="test-instagram-client-id"
INSTAGRAM_CLIENT_SECRET="test-instagram-client-secret"

# Test-specific settings
NODE_ENV="test"