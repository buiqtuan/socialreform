{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "npx jest --config tests/setup/jest.config.js",
    "test:integration": "npx jest --config tests/setup/jest.config.js",
    "test:watch": "npx jest --config tests/setup/jest.config.js --watch",
    "test:coverage": "npx jest --config tests/setup/jest.config.js --coverage",
    "clean": "rm -rf .next dist"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@social-reform/shared-types": "workspace:*",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "firebase-admin": "^12.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}